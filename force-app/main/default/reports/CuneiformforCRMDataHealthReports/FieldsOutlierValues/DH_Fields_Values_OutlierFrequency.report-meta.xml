        <isCrossBlock>false</isCrossBlock>
        <masterLabel>Total Outliers per Field</masterLabel>
        <scale>0</scale>
    </aggregates>
    <aggregates>
        <calculatedFormula>IF (
    OR(
        pnova__Profiling_Value_Results__c.pnova__Prf_TotalRecords_Populated_SetA__c:AVG = 0,
        pnova__Profiling_Value_Results__c.pnova__ProfilingFieldResults__c.pnova__Prf_TotalRecords_Distinct_SetA__c:AVG = 0
    ),
    NULL,
    IF (
        pnova__Profiling_Value_Results__c.pnova__ProfilingFieldResults__c.pnova__Prf_TotalRecords_Distinct_SetA__c:AVG = 1,
        1,
        IF (
            pnova__Profiling_Value_Results__c.pnova__Prf_TotalRecords_Populated_SetA__c:AVG / pnova__Profiling_Value_Results__c.pnova__ProfilingFieldResults__c.pnova__Prf_TotalRecords_Distinct_SetA__c:AVG = 0,
            NULL,
            ROUND(pnova__Profiling_Value_Results__c.pnova__Prf_TotalRecords_Populated_SetA__c:AVG / (FLOOR(
                pnova__Profiling_Value_Results__c.pnova__ProfilingFieldResults__c.pnova__Prf_TotalRecords_Populated_SetA__c:SUM / pnova__Profiling_Value_Results__c.pnova__ProfilingFieldResults__c.pnova__Prf_TotalRecords_Distinct_SetA__c:AVG
            )),2)
        )
    )
)</calculatedFormula>
